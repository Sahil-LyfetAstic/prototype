                        </li>
                        <li class="breadcrumb-item bcrumb-1">
                            <a href="../../index.html">
                                <i class="fas fa-home"></i> Resurs</a>
                        </li>
                        <li class="breadcrumb-item active">Add Main Service</li>
                    </ul>
                </div>
            </div>
        </div>
        <br>
        <br>
        <div class="row clearfix" id="add-us">
            <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                <div class="card">
                    <div class="header">
                        <h2>
                            <strong>Add</strong> users
                        </h2>
                        <h4 id="msg"></h4>
                    </div>
                    <div class="body" id="subadmin-tdl-status">
                        <div class="table-responsive">
                            <form id="add-adminuser" method="POST" , action="add-adminuser">
                                <div class="form-row">
                                    <div class="form-group col-md-6">
                                        <label for="inputusername">username</label>
                                        <input type="text" class="form-control" id="inputusername" name="username"
                                            placeholder="username">
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="inputEmail4">Email</label>
                                        <input type="email" class="form-control" id="inputEmail4" placeholder="Email"
                                            name="email">
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col-md-6">
                                        <label for="mobile">Mobile</label>
                                        <input type="tel" class="form-control" id="mobile" placeholder="Mobile"
                                            name="mobile">
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="inputPassword4">Password</label>
                                        <input type="password" class="form-control" id="inputPassword4" name="password"
                                            placeholder="Password">
                                    </div>
                                </div>


                                <div class="form-row">


                                    <div class="form-group col-md-4">
                                        <label for="inputState">Jobs</label>
                                        <select id="inputState" class="form-control" name="jobs">
                                            <option value="add-service">Add-Service</option>
                                            <option value="manager">Add-Manager</option>
                                            {{!-- <option value="add-tld">Add-TLD</option>
                                            <option value="confirm-tld">Confirm-TLD</option>
                                            <option value="add-keyword">Add-keyword</option>
                                            <option value="approvel">Approvel</option> --}}
                                        </select>
                                    </div>


                                </div>

                                <button type="submit" id="add-user" class="btn btn-primary">Add User</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="header">
                        <h2>
                            <strong>service</strong> approvel
                        </h2>
                      
                    </div>
                            <div class="body" id="service_table">
                        <div class="table-responsive">
                            <table class="table table-bordered  table-hover js-basic-example dataTable service-approvel" id="admin-table">
                                 <thead>
                                    <tr>
                                        <th>Service Name</th>
                                        <th>Date</th>
                                        <th>Time</th>
                                        <th>Added user</th>
                                         <th>Admin update Time</th>
                                        <th>Admin update Date</th>
                                        <th>Status</th>
                                        <th>action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each service}}
                                    <tr>
                                        <td>{{this.service}}</td>
                                        <td>{{this.addDate}}</td>
                                        <td>{{this.addTime}}</td>
                                        <td>{{this.updateUser}}</td>
                                         <td>{{this.adminUpdateTime}}</td>
                                        <td>{{this.adminUpdatedDate}}</td>
                                        <td>{{this.status}}</td>
                                        <td>
                                    
                                         
                                            <button class="btn  btn-outline-success" value="{{this.btn1}}" id="{{this._id}}" onclick="updateStatus(event)">
                                                {{this.btn1}}
                                            </button>
                                            <button class="btn  btn-outline-danger" value="{{this.btn2}}" id="{{this._id}}" onclick="updateStatus(event)">
                                                {{this.btn2}}
                                            </button>
                                        
                                        </td>  
                                    

                                    </tr>
                                    {{/each}}

                                    </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="header">
                        <h2>
                            <strong>password changing</strong> Requests
                        </h2>
            
                    </div>
                    <div class="body" id="reset-password-approvel">
                        <div class="table-responsive">
                            <table class="table table-bordered table-striped table-hover js-basic-example dataTable"
                                id="subadmin-tld">
                                <thead>
                                    <tr>
                                        <th>User Name</th>
                                        <th>Email</th>
                                        <th>Mobile</th>
                                        <th>Jobs</th>
                                        <th>Status</th>
                                        <th>Action</th>

                                </thead>
                                <tbody>
                                    {{#each passUsers}}
                                    <tr>
                                        <td>{{this.username}}</td>
                                        <td>{{this.email}}</td>
                                        <td>{{this.mobile}}</td>
                                        <td>{{this.jobs}}</td>
                                        <td>{{this.status}}</td>
                                        <td>
                                            
                                            <button class="btn  btn-outline-success" value="approved" id="{{this._id}}"
                                                onclick="resetPasswordAprovel(event)">
                                                Approve
                                            </button>
                                            <button class="btn  btn-outline-danger" value="rejected" id="{{this._id}}"
                                                onclick="resetPasswordAprovel(event)">
                                                Reject
                                            </button>
                                        </td>


                                    </tr>
                                    {{/each}}

                                    </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>