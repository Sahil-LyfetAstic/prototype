<section class="content" onload="tes()">
    <div class="container-fluid">
        <div class="block-header">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <ul class="breadcrumb breadcrumb-style ">
                        <li class="breadcrumb-item">
                            <h4 class="page-title">Sub Admin Home</h4>
                        </li>
                        <li class="breadcrumb-item bcrumb-1">
                            <a href="../../index.html">
                                <i class="fas fa-home"></i> Resurs</a>
                        </li>
                        <li class="breadcrumb-item active">Add Main Service</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row" onload="setStyle(event)">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="card">
                    <div class="body">

                        <div class="table-responsive">
                            <div class="">
                                <h5 id="uploadmsg" style="display: none;"></h5>
                            </div>
                            <br>
                            <form id="add-keyword-form">
                                <div class="form-group">
                                    <h5>Upload Keyword</h5>
                                    <br>
                                    <select class="form-select col-md-3" id="collection-drop" name="coll">
                                        {{#each service}}
                                        <option value="{{this._id}}">{{this.keyword_collection}}</option>
                                        {{/each}}
                                    </select>

                                </div>

                                <div class="form-group">
                                    <label for="myFile">upload file</label>
                                    <input type="file" id="keyword-file" name="myfile">

                                </div>



                                <button type="submit" id="add-key" class="btn btn-primary">Upload</button>
                            </form>



                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row" onload="setStyle(event)">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="card">
                    <div class="body">
                        <div class="table-responsive">

                            <div class="alert">
                                <span style="color: red; padding-left:250px" id="errMsgsubcat"></span>
                                <span style="color:green; padding-left:250px" id="success-msg"></span>
                            </div>


                            <div class="row" id="drag">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <h5>select service</h5>
                                        <select class="form-select" id="ser-key">
                                            {{#each service}}
                                            <option value="{{this._id}}">{{this.keyword_collection}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">

                                        <h5 id="afteruploadmsg" style="display:none;"></h5>
                                    </div>
                                </div>
                            </div>

                            <div class="row lists" id="tst">
                                <div class="col-md-4">

                                    <div class="list" id="dis">

                                        {{#each keywords}}




                                        <div class="accordion" name="tst" id="d{{this._id}}" class="example-draggable"
                                            draggable="true" ondragstart="onDragStart(event);">
                                            <input style="display: none;" type="text" name="cat" id="d{{this._id}}i"
                                                value="{{this.keyword_name}}">
                                            <div class="accordion-item" id="tesssst">
                                                <h2 class="accordion-header" id="headingTwo"> <button
                                                        value="{{this.keyword_name}}" id="d{{this._id}}b"
                                                        class="accordion-button collapsed" type="button"
                                                        data-bs-toggle="collapse" data-bs-target="#d{{this._id}}t"
                                                        aria-expanded="false" aria-controls="d{{this._id}}t">
                                                        <i id="add-b" class="fas fa-plus-circle add-b"
                                                            style="padding: 10px;"></i>
                                                        {{this.keyword_name}}<i id="bb" class="fas fa-plus-circle add-a"
                                                            style="padding: 10px;" ondragover="onDragOver(event);"
                                                            ondrop="onDrop(event);"></i>
                                                             <i class="fas fa-pen" style="padding: 10px;"
                                                            onclick="editKey(this)"></i>
                                                        <i class="fas fa-minus-circle" style="padding: 10px;"
                                                            onclick="delKey(this)"></i>
                                                        <br>
                                                       
                                                    </button><input sty type="text" value="{{this.keyword_name}}"
                                                        style="display: none !important;"></h2>


                                            </div>

                                        </div>
                                        <br>


                                        {{/each}}


                                    </div>
                                </div>


                                <div class="col-md-4">
                                    <form action="relation-form" id="relation-form">
                                        <div class="list list2" id="list2" ondragover="onDragOver(event);"
                                            ondrop="onDrop(event);">


                                        </div>
                                        <button id="relation-submit">submit</button>

                                    </form>

                                </div>
                            </div>
                            <div class="modal" tabindex="-1" role="dialog">
                                <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5 class="modal-title">Modal title</h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div class="modal-body">
                                            <input type="text" name="modal-input" id="modal-input" value="">
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-primary" id="modal-save">Save
                                                changes</button>
                                            <button type="button" class="btn btn-secondary" id="modal-close"
                                                data-dismiss="modal">Close</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


</section>