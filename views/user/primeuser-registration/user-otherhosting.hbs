<form action="primeuser-register" method="POST" id="prime-user-register">
    <div class="input-group" >
        <input class="form-control" type="text" name="Domain" style="width: 100px;" placeholder="Domain" id="Domain"
            onkeyup="blockChar(this)"  />

        <select name="domainExtension" id="extention"
            style="background-color:#737373;padding-left:20px;border-radius: 8px;">
            {{!-- {{#each data}}
            <option value="{{this.TLD_NAME}}">{{this.TLD_NAME}}</option>
            {{/each}} --}}
            <option value="com">com</option>
            <option value="in">in</option>
            <option value="org">org</option>
        </select />

    </div>
    <br>
    <div class="input-group mb-3">
        <input type="text" class="form-control" name="ed" placeholder="Domain under email"
            aria-label="Recipient's username" id="ed" aria-describedby="basic-addon2">
        <div class="input-group-append">
            <span class="input-group-text" id="basic-addon2"></span>
        </div>
    </div>




    <br>
    <input class="form-control" type="text" name="fname" id="fname" placeholder="First Name" required>
    <input class="form-control" type="text" name="lname" id="lname" placeholder=" Last Name" required>

    <input class="form-control" type="email" name="aditionalemail" placeholder="Aditional Email Address" required>
    <input type="tel" id="phone" name="phone" placeholder="Mobile number" required>



    <button type="submit" class="btn btn-primary" id="primeuser-submit">Submit</button>
</form>